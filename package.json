{
  "name": "lsc-questing-dcl",
  "version": "1.1.8",
  "description": "SDK7 library to connect Decentraland scenes to the custom Quest System",
  "main": "./dist/index.js",
  "scripts": {
    "build": "rollup -c --environment BUILD:production && tsc -p tsconfig.json",
    "link": "npm link && rollup --watch -c --environment BUILD:production"
  },
  "typings": "./dist/index.d.ts",
  "decentralandLibrary": {
    "main": "./dist/index.js"
  },
  "keywords": [
    "dcl",
    "decentraland",
    "sdk",
    "library"
  ],
  "author": "-",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/last-slice/lsc-questing-dcl.git"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@dcl/js-runtime": "^7.8.0",
    "@dcl/sdk": "^7.8.0",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "@dcl-sdk/utils": "latest",
    "colyseus.js": "^0.15.10",
    "mitt": "^3.0.1",
    "@types/node": ">=18.0.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0",
    "yarn": "please use npm"
  }
